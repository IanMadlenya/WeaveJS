{
  "compilerOptions": {
    "noImplicitAny": true,
    "jsx": "preserve",
    "removeComments": false,
    "target": "es6",
    "outDir": "outts",
    "inlineSources": true,
    "inlineSourceMap": true,
    "declaration": false,
	"forceConsistentCasingInFileNames": true,
	"listFiles": true,
	"noImplicitReturns": false,
	"pretty": false
  },
  "exclude":[
    "node_modules", "outts", "lib-typings", "WeaveASJS"
  ],
  "compileOnSave": false
}
