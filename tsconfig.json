{
  "compilerOptions": {
    "noImplicitAny": true,
    "jsx": "preserve",
    "removeComments": false,
    "target": "es6",
    "module": "amd",
    "outFile": "outts/weavejs.js",
    "inlineSources": true,
    "inlineSourceMap": true,
    "declaration": true,
	"forceConsistentCasingInFileNames": true,
	"listFiles": false,
	"noImplicitReturns": false,
	"pretty": false
  },
  "files": [
  	"srcts/_lib-references.ts",
  	"srcts/_references.ts"
  ],
  "compileOnSave": false
}
