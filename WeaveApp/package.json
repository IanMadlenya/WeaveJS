{
  "name": "example",
  "version": "0.0.0",
  "description": "Collection of visualization tools and UI for the WeaveJS project.",
  "scripts": {
    "clean": "rimraf es6/ && rimraf js/ && rimraf dist/bundle.js",
    "babel": "babel es6 -d js --source-maps inline",
    "browserify": "mkdirp dist && browserify js/index.js -o dist/bundle.js",
    "copy-typings": "mkdirp typings/weave && cp ../weave-typings/* typings/weave",
    "compile": "npm run clean && npm run copy-typings && tsc && npm run babel && npm run browserify && cd .. && node run-ts copy.ts"
  },
  "main": "js/app/WeaveApp.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/WeaveTeam/WeaveJS.git"
  },
  "keywords": [
    "visualization",
    "weave"
  ],
  "author": "WeaveTeam <devteam@iweave.com>",
  "license": "MPL-2.0",
  "dependencies": {
    "babel-cli": "^6.5.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "browserify": "^13.1.0",
    "c3": "https://github.com/WeaveTeam/c3.git",
    "clipboard": "^1.5.10",
    "codemirror": "^5.17.0",
    "d3": "^3.5.9",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "openlayers": "3.13.0",
    "pixi.js": "^3.0.11",
    "proj4": "^2.3.14",
    "rc-slider": "^3.6.2",
    "react": "^0.14.7",
    "react-codemirror": "^0.2.6",
    "react-color": "^2.0.0",
    "react-dom": "^0.14.7",
    "react-dropzone": "^3.4.0",
    "react-notification-system": "^0.2.7",
    "react-sparklines": "^1.5.2",
    "rimraf": "^2.5.3",
    "typescript": "^1.8.10",
    "weavejs": "../"
  },
  "devDependencies": {
    "mkdirp": "^0.5.1"
  }
}
