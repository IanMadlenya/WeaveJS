{
  "name": "WeaveJS",
  "version": "0.0.0",
  "description": "Collection of visualization tools and UI for the WeaveJS project.",
  "main": "lib/WeaveJS.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dts": "tsc -p tsconfig-definitions.json",
    "build": "tsc && babel -d lib outts",
    "compile": "npm run build && browserifyinc -x react -x react-dom -x jquery -x lodash -x d3 -x c3 -x openlayers -x jszip --debug --cachefile browserifycache.json lib/index.js > dist/index.js",
    "compile-libs": "browserifyinc -r react -r react-dom -r jquery -r lodash -r d3 -r c3 -r openlayers -r jszip --debug --cachefile browserifycache.json > dist/libs.js",
    "postinstall": "cd lib || npm run build",
    "_compile": "grunt ts && grunt babel",
    "_prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WeaveTeam/WeaveJS.git"
  },
  "keywords": [
    "visualization",
    "weave"
  ],
  "author": "WeaveTeam <devteam@iweave.com>",
  "license": "MPL-2.0",
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "grunt": "^0.4.5",
    "grunt-babel": "^6.0.0",
    "grunt-browserify": "^4.0.1",
    "grunt-contrib-clean": "^0.7.0",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-minifyify": "^3.2.1",
    "grunt-ts": "^5.3.2",
    "minifyify": "^7.1.0",
    "tsd": "^0.6.5"
  },
  "peerDependencies": {
    "react": "0.14.7",
    "react-dom": "0.14.7"
  },
  "dependencies": {
    "babel": "^6.1.18",
    "babel-cli": "^6.4.5",
    "browserify-incremental": "^3.1.1",
    "c3": "https://github.com/WeaveTeam/c3.git",
    "classnames": "^2.2.3",
    "clipboard": "^1.5.10",
    "d3": "^3.5.9",
    "filesaver.js": "^0.2.0",
    "fixed-data-table": "^0.6.0",
    "font-awesome": "^4.4.0",
    "jquery": "^2.1.4",
    "jszip": "^3.0.0",
    "lodash": "^3.10.1",
    "moment": "^2.11.2",
    "openlayers": "^3.13.0",
    "proj4": "^2.3.14",
    "rc-slider": "^3.6.2",
    "react-addons-update": "0.14.7",
    "react-color": "^2.0.0",
    "react-dropzone": "^3.4.0",
    "react-sparklines": "^1.5.2",
    "typescript": "^1.8.0"
  }
}
